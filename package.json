{
  "name": "intrepid",
  "version": "1.0.0",
  "description": "Back-End Engineer Technical Assessment",
  "main": "./build/src/index.js",
  "scripts": {
    "build": "tsc -p .",
    "start": "node ./build/src/index.js",
    "dev": "nodemon",
    "lint": "eslint --fix .",
    "pretest": "NODE_ENV=test sequelize-cli db:create; NODE_ENV=test sequelize-cli db:migrate",
    "test": "NODE_ENV=test PORT=8080 jest --runInBand --coverage",
    "posttest": "NODE_ENV=test sequelize-cli db:drop",
    "prepare": "husky install",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "db:migrate": "sequelize-cli db:migrate",
    "db:rollback": "sequelize-cli db:migrate:undo",
    "db:reset": "sequelize-cli db:migrate:undo:all",
    "db:generate:migration": "sequelize-cli migration:generate --name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rrandall91/intrepid.git"
  },
  "author": "Rashaad R. Randall <hello@rashaadrandall.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rrandall91/intrepid/issues"
  },
  "homepage": "https://github.com/rrandall91/intrepid#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-node": "^11.1.0",
    "faker": "^5.4.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "pg": "^8.5.1",
    "sequelize": "^6.5.1",
    "sequelize-cli": "^6.2.0",
    "slugify": "^1.4.7"
  }
}
